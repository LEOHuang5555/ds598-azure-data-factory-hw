{
	"name": "RestResource1",
	"properties": {
		"linkedServiceName": {
			"referenceName": "RestOpenWeather",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"lat": {
				"type": "float",
				"defaultValue": 30.7918613
			},
			"lon": {
				"type": "float",
				"defaultValue": -83.7898868
			},
			"start": {
				"type": "string",
				"defaultValue": "@ticks(addDays(utcNow(), mul(-7, item())))"
			},
			"end": {
				"type": "string",
				"defaultValue": "@ticks(addDays(utcNow(), mul(-7, add(item(), 1))))"
			},
			"apiKey": {
				"type": "string",
				"defaultValue": "@activity('Web1').output.value"
			}
		},
		"annotations": [],
		"type": "RestResource",
		"typeProperties": {
			"relativeUrl": {
				"value": "data/2.5/history/city?lat=@{dataset().lat}&lon=@{dataset().lon}&type=hour&start=@ticks(addDays(utcNow(), mul(-7, item())))&end=@ticks(addDays(utcNow(), mul(-7, add(item(), 1))))&appid=@{dataset().apiKey}\n",
				"type": "Expression"
			}
		},
		"schema": []
	}
}