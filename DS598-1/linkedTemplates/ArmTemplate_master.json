{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DS598-1"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"GitHubDS598_credential": {
			"type": "secureString",
			"metadata": "Secure string for 'credential' of 'GitHubDS598'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageds598.dfs.core.windows.net/"
		},
		"GitHubDS598_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "LEOHuang5555"
		},
		"OWAPIkey_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://DS598OWAPI.vault.azure.net/"
		},
		"RestOpenWeather_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://history.openweathermap.org/"
		},
		"RestOpenWeather_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "RenkH"
		},
		"RestOpenWeather2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://api.openweathermap.org/"
		},
		"RestOpenWeather2_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "RenkH"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DS598-1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"GitHubDS598_credential": {
						"value": "[parameters('GitHubDS598_credential')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"GitHubDS598_properties_typeProperties_username": {
						"value": "[parameters('GitHubDS598_properties_typeProperties_username')]"
					},
					"OWAPIkey_properties_typeProperties_baseUrl": {
						"value": "[parameters('OWAPIkey_properties_typeProperties_baseUrl')]"
					},
					"RestOpenWeather_properties_typeProperties_url": {
						"value": "[parameters('RestOpenWeather_properties_typeProperties_url')]"
					},
					"RestOpenWeather_properties_typeProperties_userName": {
						"value": "[parameters('RestOpenWeather_properties_typeProperties_userName')]"
					},
					"RestOpenWeather2_properties_typeProperties_url": {
						"value": "[parameters('RestOpenWeather2_properties_typeProperties_url')]"
					},
					"RestOpenWeather2_properties_typeProperties_userName": {
						"value": "[parameters('RestOpenWeather2_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}